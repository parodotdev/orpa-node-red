{
    "LineCount.excludes": [
        "**/.vscode/**",
        "**/node_modules/**",
        "**/test/**",
        "**/vendor/**",
        "**/public/**",
        "**/locales/**",
        "**/*package.json",
        "**/*lock.json",
        "**/*lerna.json",
        "**/*LICENSE*",
        "**/*.md",
        "**/count/**",
        "**/*.log",
        "**/*.png",
